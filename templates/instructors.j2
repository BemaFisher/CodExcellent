<html>
<head>
<title>Instructors</title>
<link href="style.css" rel="stylesheet" type="text/css" />
</head>

<body>
  <h1>Instructors</h1>

  [ <a href="students">Students</a> | <a href="student_enrollments">Student Enrollments</a> | <a href="instructors">Instructor</a> | <a href="courses">Courses</a> | <a href="certificates">Certificates</a> ]
<!-- browse -->
  <div id="browse">
  <table border="1" cellpadding="5">
      <thead>
      <tr>
         <th>Instructors ID</th>
         <th>Name</th>
         <th>Email</th>
         <th>Phone number</th>
          <th>Instructor itle</th>
         <th>Pronoun</th>
      </tr>
      </thead>
    <tbody>
    {% for instructor in instructors %}
        <tr>
            <td>{{instructor['instructor_id']}}</td>
            <td>{{instructor['name']}}</td>
            <td>{{instructor['email']}}</td>
            <td>{{instructor['phone_number']}}</td>
            <td>{{instructor['instructor_title']}}</td>
            <td>{{instructor['pronoun']}}</td>
        </tr>
    {% endfor %}
    </tbody>
  </table>
  <p>&nbsp;</p>
  </div>
  <!-- insert -->
  <div id="insert">
      <form method="POST">
          <legend><strong>Add Instructors</strong></legend>
            <fieldset class="fields">
                <label>Name</label> <input type="text" name="instructor_name">
                <label>Email</label> <input type="text" name="instructor_email">
                <label>Phone number</label> <input type="text" name="instructor_phone_number">
                <label>Instructor Title</label> <input type="text" name="instructor_title">
                <label>Pronoun</label> <input type="text" name="instructor_pronoun">
            </fieldset>
            <input class="btn" type="submit" value="Add Instructor">
            <input class="btn" type="button" value="Cancel">
    </form>
  </div>
  <p>&nbsp;</p>
  <!-- update -->
  <div id="update">
      <form method="PUT">
          <legend><strong>Update Instructor Information</strong></legend>
            <fieldset class="fields">
                <label>Name</label> <input type="text" name="instructor_name">
                <label>Email</label> <input type="text" name="instructor_email">
                <label>Phone number</label> <input type="text" name="instructor_phone_number">
                <label>Instructor Title</label> <input type="text" name="instructor_title">
                <label>Pronoun</label> <input type="text" name="instructor_pronoun">
            </fieldset>
            <input class="btn" type="submit" value="Update">
            <input class="btn" type="button" value="Cancel">
    </form>
  </div>
  <p>&nbsp;</p>
  <!-- delete -->
  <div id="delete" style="display: block">
    <form method="DELETE">
        <legend><strong>Delete Instructor</strong></legend>
            <fieldset class="fields">
            <p>Are you sure you want to delete this Instructor?</p>
            <input type="hidden" name="instructor_id" value="1">
            <label>Instructor ID</label> <input type="text" name="instructor_id">

      </fieldset>
      <input class="btn" type="submit" value="Delete">
      <input class="btn" type="button" value="cancel">
    </form>
  </div>
</body>
</html>